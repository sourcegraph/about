{
  "name": "about-replatform",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn run eslint && yarn run typecheck",
    "typecheck": "tsc -p .",
    "eslint": "eslint '**/*.{ts,tsx}'",
    "ci": "yarn install --immutable --immutable-cache --check-cache"
  },
  "eslintConfig": {
    "extends": "@sourcegraph/eslint-config",
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "rules": {
      "import/no-default-export": "off"
    }
  },
  "dependencies": {
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@sourcegraph/eslint-config": "^0.25.1",
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.10",
    "typescript": "4.5.5"
  }
}
