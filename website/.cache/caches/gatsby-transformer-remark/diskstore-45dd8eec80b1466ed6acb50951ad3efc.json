olumn":4,"offset":6035},"end":{"line":44,"column":71,"offset":6102},"indent":[]}}],"position":{"start":{"line":44,"column":4,"offset":6035},"end":{"line":44,"column":71,"offset":6102},"indent":[]}}],"position":{"start":{"line":44,"column":1,"offset":6032},"end":{"line":44,"column":71,"offset":6102},"indent":[]}},{"type":"listItem","spread":false,"checked":null,"children":[{"type":"paragraph","children":[{"type":"text","value":"Refactor code to replace insecure functions, or update vulnerable packages.","position":{"start":{"line":45,"column":4,"offset":6106},"end":{"line":45,"column":79,"offset":6181},"indent":[]}}],"position":{"start":{"line":45,"column":4,"offset":6106},"end":{"line":45,"column":79,"offset":6181},"indent":[]}}],"position":{"start":{"line":45,"column":1,"offset":6103},"end":{"line":45,"column":79,"offset":6181},"indent":[]}}],"position":{"start":{"line":41,"column":1,"offset":5795},"end":{"line":45,"column":79,"offset":6181},"indent":[1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"Batch Changes brings visibility and automation to codebase-wide changes. It removes the repetitive work of manually executing code change tools over many repositories and tracking progress in spreadsheets. With Batch Changes, large-scale code changes become much easier to manage, which promotes a healthier, more up-to-date codebase.","position":{"start":{"line":47,"column":1,"offset":6183},"end":{"line":47,"column":335,"offset":6517},"indent":[]}}],"position":{"start":{"line":47,"column":1,"offset":6183},"end":{"line":47,"column":335,"offset":6517},"indent":[]}},{"type":"paragraph","children":[{"type":"strong","children":[{"type":"text","value":"Try it for yourself! Enterprise users can create batch changes with up to 5 changesets for free using our ","position":{"start":{"line":49,"column":3,"offset":6521},"end":{"line":49,"column":109,"offset":6627},"indent":[]}},{"type":"link","title":null,"url":"https://docs.sourcegraph.com/batch_changes/quickstart","children":[{"type":"text","value":"Quickstart for Batch Changes guide","position":{"start":{"line":49,"column":110,"offset":6628},"end":{"line":49,"column":144,"offset":6662},"indent":[]}}],"position":{"start":{"line":49,"column":109,"offset":6627},"end":{"line":49,"column":200,"offset":6718},"indent":[]}},{"type":"text","value":".","position":{"start":{"line":49,"column":200,"offset":6718},"end":{"line":49,"column":201,"offset":6719},"indent":[]}}],"position":{"start":{"line":49,"column":1,"offset":6519},"end":{"line":49,"column":203,"offset":6721},"indent":[]}}],"position":{"start":{"line":49,"column":1,"offset":6519},"end":{"line":49,"column":203,"offset":6721},"indent":[]}},{"type":"paragraph","children":[{"type":"strong","children":[{"type":"link","title":null,"url":"http://about.sourcegraph.com/contact/request-batch-changes-demo","children":[{"type":"text","value":"Request a demo","position":{"start":{"line":51,"column":4,"offset":6726},"end":{"line":51,"column":18,"offset":6740},"indent":[]}}],"position":{"start":{"line":51,"column":3,"offset":6725},"end":{"line":51,"column":84,"offset":6806},"indent":[]}},{"type":"text","value":" and learn more about adding Sourcegraph Batch Changes to your Sourcegraph instance.","position":{"start":{"line":51,"column":84,"offset":6806},"end":{"line":51,"column":168,"offset":6890},"indent":[]}}],"position":{"start":{"line":51,"column":1,"offset":6723},"end":{"line":51,"column":170,"offset":6892},"indent":[]}}],"position":{"start":{"line":51,"column":1,"offset":6723},"end":{"line":51,"column":170,"offset":6892},"indent":[]}}],"position":{"start":{"line":1,"column":1,"offset":0},"end":{"line":52,"column":1,"offset":6893}}}}